    "plaintext": "html"
  },
  "tailwindCSS.experimental.classRegex": [
    "class:\\\\s*?[\\\"\\\']([^\\\"\\\']*)[\\\"\\\']",
    "(?:enter|leave)(?:From|To)?=\\\\s*?[\\\"\\\']([^\\\"\\\']*)[\\\"\\\']",
    "(?:enter|leave)(?:From|To)?=\\\\s*?\\\\{([^\\\\}]*)\\\\}",
    ":\\\\s*?[\\\"\\\']([^\\\"\\\']*)[\\\"\\\']"
  ],
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "editor.quickSuggestions": {
