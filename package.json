    "lighthouse:desktop": "lhci autorun",
    "audit:performance": "npm run build && npm run lighthouse",
    "audit:mobile": "npm run build && npm run lighthouse:mobile",
    "audit:full": "npm run audit:performance && npm run audit:mobile",
    "analyze:bundle": "ANALYZE=true npm run build",
    "analyze:deps": "npx depcheck",
    "analyze:size": "npx size-limit",
    "perf:test": "npm run build && npm run start & sleep 10 && npm run lighthouse && kill %1",
    "vitals:check": "node scripts/check-web-vitals.js"
