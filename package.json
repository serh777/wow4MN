  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "clean": "rm -rf .next node_modules out",
    "build:indexer": "tsc --project tsconfig.indexer.json",
    "indexer:build": "tsc -p tsconfig.node.json",
    "indexer:start": "node dist/indexer/main.js",
